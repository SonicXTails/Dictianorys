<Application x:Class="WorkWithLibs.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WorkWithLibs"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/ThemesLib;component/Themes/Dark.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/ThemesLib;component/Themes/Light.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/ThemesLib;component/Themes/Ruby.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
